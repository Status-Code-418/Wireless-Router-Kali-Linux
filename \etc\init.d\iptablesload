#!/bin/sh
### BEGIN INIT INFO
# Provides:          iptablesload
# Required-Start:    $local_fs $network
# Required-Stop:     $local_fs $network
# Default-Start:     2 3 4 5
# Default-Stop:      0 1 6
# Short-Description: Load iptables rules
### END INIT INFO

/sbin/iptables-restore < /etc/iptables/rules.v4
